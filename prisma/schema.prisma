generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  password String
  username String
}

model Friends{
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  friendid String
  userid String
  blocked Strings
}