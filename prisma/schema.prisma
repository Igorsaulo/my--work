generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  email        String @unique
  password     String
  username     String
}

model Friends {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  friendid String @db.ObjectId
  userid   String @db.ObjectId
  blocked  String
}

model Photo {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  userId String @db.ObjectId
  photo  String 
}
model ProfilePhoto{
   id     String @id @map("_id") @db.ObjectId
  profilephoto String
}