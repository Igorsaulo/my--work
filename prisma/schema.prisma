generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  email        String @unique
  password     String
  username     String
  bio          String?
  profilephoto String? @default("https://upload.wikimedia.org/wikipedia/commons/7/72/Default-welcomer.png")
  friends Json?
  blocked Json?
  photos Json?
}
